<div class="row">
  <div class ="col s12">
      <h3 class="center-align">Students Directory</h3>
     </div> 
  </div>   



  <div class="row">
      <% @students.all.each do |student| %>
        <div class="col s6 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
            <span class="card-title">NAME:<%= student.name %></span>
            <pclass="card-title">EMAIL:<%= student.email %></p>
            <p> Course Enrollments:none </p>
            </div>
            <div class="card-action">
            <a href="#">view profile</a>
            </div>
        </div>
        </div>
      <%end%>
 </div>


 